<?xml version="1.0"?>
<extension key="de.systopia.resource" type="module">
  <file>resource</file>
  <name>CiviCRM Resource Management</name>
  <description>A framework allowing definition, management, allocation, and optimisation of resources</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.resource</url>
    <url desc="Documentation">https://docs.civicrm.org/resource/de/latest</url>
    <url desc="Support">https://github.com/systopia/de.systopia.resource/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate/>
  <version>1.0-dev</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>5.65</ver>
  </compatibility>
  <comments/>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/Resource</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>entity-types-php@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_Resource_Upgrader</upgrader>
</extension>
